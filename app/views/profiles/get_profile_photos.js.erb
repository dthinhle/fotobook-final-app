<% if @mode == "photos" %>
$(".collection").html('<%= j (render partial: "profiles/profile_photos", locals: {profile: @user, page: 1}) %>')
<% elsif @mode == "albums" %>
$(".collection").html('<%= j (render partial: "profiles/profile_albums", locals: {profile: @user, album: @album, page: 1}) %>')
<% end %>