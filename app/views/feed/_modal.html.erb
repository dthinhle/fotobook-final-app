<!-- Modal -->
<div class="modal fade" id="imgPreviewModal" tabindex="-1" role="dialog" aria-labelledby="imgPreviewHome" aria-hidden="true">
  <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="imgPreview">Title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="img-preview-body">
        <%= image_tag  "dummy-photo.jpg" , class: 'img-fluid photoPreview' %>
        <div class="carousel slide albumPreview" data-ride="carousel">
          <ol class="carousel-indicators">
            <li data-target=".albumPreview" data-slide-to="0" class="active"></li>
            <li data-target=".albumPreview" data-slide-to="1"></li>
          </ol>
          <div class="carousel-inner" role="listbox">
            <div class="carousel-item active">
              <%= image_tag  "001", class: 'img-fluid' %>
            </div>
            <div class="carousel-item">

              <%= image_tag  "002", class: 'img-fluid' %>
            </div>
          </div>
          <a class="carousel-control-prev" href=".albumPreview" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href=".albumPreview" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>


      <div class="modal-footer justify-content-start">
        <div>
          <span class="text-primary ml-2"></span>
        <hr>
          <p class="text-secondary">
          </p>
        </div>
      </div>
    </div>
  </div>
</div>